<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title><%= it.title %> &mdash; Trace Collector</title>
    <link rel="shortcut icon" href="/static/favicon.ico" />

    <link rel="stylesheet" href="/static/bundle.css" type="text/css" />

    <!-- Google Ajax loader -->
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>

    <script type="text/javascript" src="/static/bundle.js"></script>

    <%~ it.extrahead || "" %>
  </head>
  <body>
    <div id="navbar" class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="/">Trace Collector</a>
        </div>
      </div>
    </div>

    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 col-md-2 sidebar"><%~ it.navbar || "" %></div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <h1 id="page-header" class="page-header">
            <%= it.pagetitle %>
            <span class="small text-muted pull-right">
              <%= it.pagesubtitle || "" %>
            </span>
          </h1>

          <%~ it.body %>
        </div>
      </div>
      <!-- row -->
    </div>
    <!-- container-fluid -->
  </body>
</html>
